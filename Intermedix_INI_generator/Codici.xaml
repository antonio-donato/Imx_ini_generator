<Window x:Class="Intermedix_INI_generator.Codici"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Intermedix_INI_generator"
        mc:Ignorable="d"
        Title="Codici" Height="300" Width="300">
    <Grid>
        <DataGrid x:Name="dataGridCodici" HorizontalAlignment="Left" Height="218" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" RowHeight="20">
            <DataGridTextColumn Header="Codice" IsReadOnly="False" Binding="{Binding Path=Test1}" Width="50"></DataGridTextColumn>
        </DataGrid>
        <Button x:Name="btnAddCode" Content="Aggiungi codice" HorizontalAlignment="Left" Margin="10,228,0,0" VerticalAlignment="Top" Width="126" Click="btnAddCode_Click"/>
        <Button x:Name="btnEnd" Content="Fine" HorizontalAlignment="Left" Margin="156,228,0,0" VerticalAlignment="Top" Width="126"/>
    </Grid>
</Window>
